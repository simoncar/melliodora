	"scripts": {
		"test": "jest --watchAll --collectCoverage",
		"ci": "jest",
		"app": "cp apps/app.ais.json app.json && expo r -c",
		"g": "expo start --config apps/app.generic.json --no-minify --ios",
		"t": "expo start --config apps/app.generic.json --no-minify --tunnel",
		"w": "expo start --config apps/app.generic.json --no-minify --web",
		"offline": "expo start --config apps/app.generic.json --no-minify --ios  --dev --offline",
		"s": "expo start",
		"a": "expo start --config apps/app.ais_edu_sg.json --no-minify --ios",
		"modules": "rm -rf node_modules && yarn",
		"p": "expo start --config apps/app.polo_photos.json --no-minify --dev --clear"
	},
